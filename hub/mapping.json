{
  "rules": [
    {
      "if_present": "whatsapp_number",
      "create_button": {
        "type": "whatsapp",
        "label": "WhatsApp Us",
        "url_template": "https://wa.me/{{value}}"
      }
    },
    {
      "if_present": "google_review_link",
      "create_button": {
        "type": "review",
        "label": "Leave a Review",
        "url_template": "{{value}}"
      }
    },
    {
      "if_present": "menu_link",
      "create_button": {
        "type": "menu",
        "label": "View Menu",
        "url_template": "{{value}}"
      }
    },
    {
      "if_present": "payment_link",
      "create_button": {
        "type": "payment",
        "label": "Pay Now",
        "url_template": "{{value}}"
      }
    }
  ]
}
